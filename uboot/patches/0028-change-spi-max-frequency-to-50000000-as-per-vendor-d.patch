From ba8f13c8fee04eca924b7837e77e3be97f98395a Mon Sep 17 00:00:00 2001
From: John Clark <inindev@gmail.com>
Date: Thu, 28 Dec 2023 11:25:56 +0000
Subject: [PATCH 28/28] change spi-max-frequency to 50000000 as per vendor
 device tree

Signed-off-by: John Clark <inindev@gmail.com>
---
 arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi b/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi
index 87831c9d43..88b3997d0a 100644
--- a/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi
+++ b/arch/arm/dts/rk3588-nanopc-t6-u-boot.dtsi
@@ -29,7 +29,7 @@
 		bootph-pre-ram;
 		compatible = "jedec,spi-nor";
 		reg = <0>;
-		spi-max-frequency = <24000000>;
+		spi-max-frequency = <50000000>;
 		spi-rx-bus-width = <4>;
 		spi-tx-bus-width = <1>;
 	};
